<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.epl603.assign2" android:versionCode="1"
	android:versionName="1.0">

	<application android:icon="@drawable/books" android:label="@string/app_name">
		<activity android:name=".ActivityBooks" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.epl603.assign2.AddBookActivity"
			android:label="@string/addbook" android:theme="@android:style/Theme.Dialog">
		</activity>
		<receiver android:name="com.epl603.classes.IncomingReceiver"
			android:enabled="true">
			<intent-filter>
				<action android:name="epl603.assignment2.ACTION_BOOK_LOOKUP_RESULT"></action>
			</intent-filter>
		</receiver>

	</application>
</manifest>